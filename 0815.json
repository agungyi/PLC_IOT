    {
        "id": "0e7d20f385c13893",
        "type": "function",
        "z": "19b63a95f6e28d2d",
        "name": "counter1",
        "func": "// Define Varialbe and get stored slow value\n// if there is no stored value, then the value is 0\nvar counter1 = flow.get(\"counter1\") || 0\n\n// add 1 to the counter value\ncounter1 ++\n\n// save the counter value in the flow variable\nflow.set(\"counter1\",counter1)\n\n// set msg.payload to counter value\nmsg.payload = counter1\n\n//return the message object\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
